<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <!-- Fundo branco com borda preta -->
  <!-- Item 1: Forma branca com borda preta, insetada -->
  <item
      android:left="8dp"
      android:top="8dp"
      android:right="8dp"
      android:bottom="8dp">
    <shape android:shape="rectangle">
      <solid android:color="#FFFFFF" />
      <!-- Borda preta, ajuste a espessura conforme necessÃ¡rio -->
      <stroke
          android:width="4dp"
          android:color="#000000" />
    </shape>
  </item>

  <!-- Logo (vector) centralizado com tamanho fixo -->
  <item
      android:drawable="@drawable/logo_5g"
      android:width="200dp"
      android:height="200dp"
      android:gravity="center"/>
</layer-list>
